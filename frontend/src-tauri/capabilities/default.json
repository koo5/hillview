{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "geolocation:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-appdata-write-recursive",
    "fs:allow-appdata-read-recursive"
  ]
}
