{
	"name": "hillview",
	"version": "0.0.1",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tauri-apps/cli": "^2.6.2",
		"@types/leaflet": "^1.9.19",
		"@wdio/appium-service": "^9.16.2",
		"@wdio/cli": "^9.16.2",
		"@wdio/local-runner": "^9.16.2",
		"@wdio/mocha-framework": "^9.16.2",
		"@wdio/spec-reporter": "^9.16.2",
		"appium": "^2.19.0",
		"appium-uiautomator2-driver": "^4.2.4",
		"appium-xcuitest-driver": "^9.9.1",
		"svelte": "^5.22.6",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"vite": "^6.3.5",
		"webdriverio": "^9.16.2"
	},
	"private": true,
	"scripts": {
		"dev": "vite dev --port 8212",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:android": "wdio run wdio.conf.ts",
